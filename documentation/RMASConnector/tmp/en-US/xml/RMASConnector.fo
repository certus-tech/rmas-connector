					</fo:table-row></fo:table-header><fo:table-body start-indent="0pt" end-indent="0pt"><fo:table-row>
						<fo:table-cell padding-left="4pt" padding-right="4pt" padding-top="2pt" padding-bottom="2pt" padding-start="2pt" padding-end="2pt" border-start-style="none" border-end-style="none" border-top-style="none" border-bottom-style="none"><fo:block>
							DatabaseClass
						</fo:block></fo:table-cell>
						 <fo:table-cell padding-left="4pt" padding-right="4pt" padding-top="2pt" padding-bottom="2pt" padding-start="2pt" padding-end="2pt" border-start-style="none" border-end-style="none" border-top-style="none" border-bottom-style="none"><fo:block>
							The database driver class name.
						</fo:block></fo:table-cell>

					</fo:table-row><fo:table-row>
						<fo:table-cell padding-left="4pt" padding-right="4pt" padding-top="2pt" padding-bottom="2pt" padding-start="2pt" padding-end="2pt" border-start-style="none" border-end-style="none" border-top-style="none" border-bottom-style="none"><fo:block>
							DatabaseUrl
						</fo:block></fo:table-cell>
						 <fo:table-cell padding-left="4pt" padding-right="4pt" padding-top="2pt" padding-bottom="2pt" padding-start="2pt" padding-end="2pt" border-start-style="none" border-end-style="none" border-top-style="none" border-bottom-style="none"><fo:block>
							The URL for the database connection.
						</fo:block></fo:table-cell>

					</fo:table-row><fo:table-row>
						<fo:table-cell padding-left="4pt" padding-right="4pt" padding-top="2pt" padding-bottom="2pt" padding-start="2pt" padding-end="2pt" border-start-style="none" border-end-style="none" border-top-style="none" border-bottom-style="none"><fo:block>
							DatabaseUsername
						</fo:block></fo:table-cell>
						 <fo:table-cell padding-left="4pt" padding-right="4pt" padding-top="2pt" padding-bottom="2pt" padding-start="2pt" padding-end="2pt" border-start-style="none" border-end-style="none" border-top-style="none" border-bottom-style="none"><fo:block>
